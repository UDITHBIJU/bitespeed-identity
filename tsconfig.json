{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
		},
        "types":["node"],
        "moduleResolution": "node",
        "resolveJsonModule": true,

	},
	"include": ["src/**/*","prisma/client.ts"],
    "exclude": ["prisma/schema.prisma", "node_modules"]
}
